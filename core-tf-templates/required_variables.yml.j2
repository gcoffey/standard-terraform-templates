{% if required.type == "string" %}
  {{ required.name }}: "<string>"       # Required; {{ required.description }}
{% endif %}
{% if required.type == "integer" %}
  {{ required.name }}: <integer>        # Required; {{ required.description }}
{% endif %}
{% if required.type == "boolean" %}
  {{ required.name }}: <boolean>        # Required; {{ required.description }}
{% endif %}
{% if required.type == "list" %}
  {{ required.name }}:  # Required; {{ required.description }}
    - <list>
{% endif %}
