resource "azurerm_app_service_custom_hostname_binding" "{{ app_service_custom_hostname_binding.name }}" {
  ## Required Vars
  hostname   = "{{ app_service_custom_hostname_binding.hostname }}"
  resource_group_name   = "{{ app_service_custom_hostname_binding.resource_group_name }}"
  app_service_name   = "{{ app_service_custom_hostname_binding.app_service_name }}"

  ## Optional Vars
  ssl_state   = "{{ app_service_custom_hostname_binding.ssl_state }}"
  thumbprint   = "{{ app_service_custom_hostname_binding.thumbprint }}"

}
