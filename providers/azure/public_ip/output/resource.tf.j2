resource "azurerm_public_ip" "{{ public_ip.name }}" {
  ## Required Vars
  location   = "{{ public_ip.location }}"
  name   = "{{ public_ip.name }}"
  resource_group_name   = "{{ public_ip.resource_group_name }}"
  allocation_method   = "{{ public_ip.allocation_method }}"

  ## Optional Vars
  zones   = "{{ public_ip.zones }}"
  ddos_protection_mode   = "{{ public_ip.ddos_protection_mode }}"
  ddos_protection_plan_id   = "{{ public_ip.ddos_protection_plan_id }}"
  domain_name_label   = "{{ public_ip.domain_name_label }}"
  edge_zone   = "{{ public_ip.edge_zone }}"
  idle_timeout_in_minutes   = "{{ public_ip.idle_timeout_in_minutes }}"
  ip_tags   = "{{ public_ip.ip_tags | to_json | safe }}"
  ip_version   = "{{ public_ip.ip_version }}"
  public_ip_prefix_id   = "{{ public_ip.public_ip_prefix_id }}"
  reverse_fqdn   = "{{ public_ip.reverse_fqdn }}"
  sku   = "{{ public_ip.sku }}"
  sku_tier   = "{{ public_ip.sku_tier }}"
  tags   = "{{ public_ip.tags | to_json | safe }}"

}
