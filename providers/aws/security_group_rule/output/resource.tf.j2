resource "aws_security_group_rule" "{{ security_group_rule.name }}" {
  ## Required Vars
  from_port   = "{{ security_group_rule.from_port }}"
  protocol   = "{{ security_group_rule.protocol }}"
  security_group_id   = "{{ security_group_rule.security_group_id }}"
  to_port   = "{{ security_group_rule.to_port }}"
  type   = "{{ security_group_rule.type }}"

  ## Optional Vars
  cidr_blocks   = "{{ security_group_rule.cidr_blocks | to_json | safe }}"
  description   = "{{ security_group_rule.description }}"
  ipv6_cidr_blocks   = "{{ security_group_rule.ipv6_cidr_blocks | to_json | safe }}"
  prefix_list_ids   = "{{ security_group_rule.prefix_list_ids | to_json | safe }}"
  self   = {{ security_group_rule.self }}
  source_security_group_id   = "{{ security_group_rule.source_security_group_id }}"

}
